{
  "$id": "https://schemas.openpoll.io/0.1/demographics.json",
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",

  "properties": {
    "ageRange": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "18 - 24",
          "25 - 32",
          "33 - 40",
          "40 - 47",
          "48 - 55",
          "56 - 70",
          "71+"
        ]
      }, {
        "type": "null"
      }]
    },

    "gender": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "male",
          "female",
          "other"
        ]
      }, {
        "type": "null"
      }]
    },

    "ethnicity": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "asian",
          "african-american",
          "caucasian",
          "hispanic",
          "native-american",
          "pacific-islander",
          "other"
        ]
      }, {
        "type": "null"
      }]
    },

    "personalIncome": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "0",
          "u25000",
          "25000-49999",
          "50000-74999",
          "75000-99999",
          "100000-249999",
          "250000-1000000",
          "1000000+"
        ]
      }, {
        "type": "null"
      }]
    },

    "householdIncome": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "0",
          "u25000",
          "25000-49999",
          "50000-74999",
          "75000-99999",
          "100000-249999",
          "250000-1000000",
          "1000000+"
        ]
      }, {
        "type": "null"
      }]
    },

    "education": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "middleschool",
          "some_highschool",
          "ged",
          "highschool",
          "trade",
          "college",
          "some_college",
          "bachelor",
          "masters",
          "doctorate"
        ]
      }, {
        "type": "null"
      }]
    },

    "politicalParty": {
      "anyOf": [{
        "type": "string",
        "emum": [
          "republican",
          "democrat",
          "independent",
          "unaffiliated"
        ]
      }, {
        "type": "null"
      }]
    },

    "votingRegistration": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "registered",
          "not_registered"
        ]
      }, {
        "type": "null"
      }]
    },

    "military": {
      "anyOf": [{
        "type": "string",
        "enum": [
          "active",
          "reserves",
          "veteran",
          "none"
        ]
      }, {
        "type": "null"
      }]
    },

    "zipcode": {
      "anyOf": [{
        "type": "integer",
        "minimum": 0,
        "maximum": 99999
      }, {
        "type": "null"
      }]
    }
  },

  "required": []
}
